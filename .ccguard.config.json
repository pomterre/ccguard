{
  "enforcement": {
    "mode": "session-wide",
    "strategy": "cumulative",
    "ignoreEmptyLines": true
  },
  "whitelist": {
    "patterns": [
      "**/test/**",
      "**/*.generated.*"
    ],
    "extensions": [
      ".md",
      ".json"
    ]
  },
  "thresholds": {
    "allowedPositiveLines": 3
  }
}
