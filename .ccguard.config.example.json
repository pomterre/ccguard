{
  "enforcement": {
    "mode": "session-wide",
    "strategy": "snapshot",
    "ignoreEmptyLines": true
  },
  "whitelist": {
    "patterns": [
      "**/node_modules/**",
      "**/dist/**",
      "**/*.generated.*",
      "**/migrations/**"
    ],
    "extensions": [
      ".md",
      ".lock",
      ".json"
    ]
  },
  "thresholds": {
    "allowedPositiveLines": 0
  }
}
